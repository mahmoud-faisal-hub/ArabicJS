{
  "name": "en-arabicjs",
  "version": "0.0.0-development",
  "description": "Package to convert English date, time, numbers, etc. to arabic and vice-versa.",
  "type": "module",
  "main": "./dist/arabic.umd.cjs",
  "module": "./dist/arabic.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/arabic.es.js",
      "require": "./dist/arabic.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^10.0.2",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/node": "^18.15.11",
    "cz-conventional-changelog": "^3.3.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "semantic-release": "^21.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-dts": "^2.1.0",
    "vitest": "^0.29.8"
  },
  "author": "Mahmoud Faisal <dev.mahmoudfaisal@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/mahmoud-faisal-hub/En-ArabicJS.git"
  },
  "bugs": {
    "url": "https://github.com/mahmoud-faisal-hub/En-ArabicJS/issues"
  },
  "keywords": [
    "En-ArabicJs",
    "arabic",
    "arabicjs",
    "english",
    "englishjs",
    "convert",
    "conversion",
    "numbers",
    "date",
    "time",
    "translate",
    "i18n"
  ],
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
